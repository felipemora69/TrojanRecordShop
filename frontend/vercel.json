{
    "version": 2,
    "builds": [
      {
        "src": "/dist/**",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "/dist"
        }
      },
      {
        "src": "api/**/*.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/dist/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/dist/index.html"
      }
    ]
  }